  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^0.11.0",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0"
  },
  "dependencies": {
    "joi": "^5.1.0",
    "request": "^2.51.0"
